{
    "extends": ["react-app", "prettier"],
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "tabWidth": 4,
                "disableLanguages": ["js"],
                "printWidth": 100
            }
        ]
    },
    "plugins": ["prettier"]
}
